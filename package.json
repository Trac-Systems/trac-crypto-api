{
  "name": "trac-crypto-api",
  "version": "0.0.1",
  "main": "index.js",
  "pear": {
    "name": "trac-crypto-api",
    "type": "terminal"
  },
  "scripts": {
    "build": "vite build",
    "test:node": "brittle-node -t 120000 test/unit/all.test.js",
    "test:bare": "brittle-bare -t 120000 test/unit/all.test.js",
    "test:browser": "npm run build && npx playwright test test/browser/all.spec.js",
    "test:all": "npm run test:node && npm run test:browser"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "util": {
      "bare": "bare-utils",
      "default": "util"
    },
    "crypto": {
      "bare": "bare-crypto",
      "default": "crypto"
    },
    "node:crypto": {
      "bare": "bare-crypto",
      "default": "crypto"
    }
  },
  "dependencies": {
    "@tracsystems/blake3": "^0.0.6",
    "b4a": "1.6.7",
    "bare-fs": "^4.0.2",
    "bech32": "^2.0.0",
    "bip39-mnemonic": "^2.2.0",
    "playwright": "^1.55.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sodium-javascript": "^0.8.0",
    "sodium-universal": "^5.0.1",
    "vite": "^7.1.3",
    "which-runtime": "^1.3.1"
  },
  "devDependencies": {
    "brittle": "^3.18.0",
    "supertest": "^7.1.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}