{
  "name": "trac-crypto-api",
  "version": "0.1.2",
  "main": "index.js",
  "pear": {
    "name": "trac-crypto-api",
    "type": "terminal"
  },
  "scripts": {
    "build:browser": "rollup -c && cd test/browser/ && npm i && cd ../..",
    "build:rn": "npm run setup:rn && webpack --config webpack.config.rn.js && npm run cleanup:rn",
    "setup:rn": "npm install crypto-browserify stream-browserify vm-browserify buffer url assert --no-save",
    "cleanup:rn": "npm uninstall crypto-browserify stream-browserify vm-browserify buffer url assert --no-save",
    "test:node": "brittle-node -t 120000 test/unit/all.test.js",
    "test:bare": "brittle-bare -t 120000 test/unit/all.test.js",
    "test:browser": "cd test/browser && npm run test:browser",
    "test:all": "npm run test:node && npm run test:browser",
    "ci:clean": "npm cache clean -f && rm -rf node_modules package-lock.json && cd test/browser/ && npm cache clean -f && rm -rf node_modules && cd ../.. && rm -rf dist && npm i",
    "ci:full": "npm run ci:clean && npm i && npm run test:node && npm run test:bare && npm run build:browser && npm run test:browser && npm run build:rn"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "util": {
      "bare": "bare-utils",
      "default": "util"
    },
    "crypto": {
      "bare": "bare-crypto",
      "default": "crypto"
    },
    "node:crypto": {
      "bare": "bare-crypto",
      "default": "crypto"
    }
  },
  "dependencies": {
    "@metamask/key-tree": "10.1.1",
    "@tracsystems/blake3": "0.0.13",
    "b4a": "1.6.7",
    "bare-fs": "4.5.0",
    "bech32": "2.0.0",
    "bip39-mnemonic": "2.5.0",
    "sodium-javascript": "0.8.0",
    "sodium-universal": "5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-classes": "^7.28.4",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "babel-loader": "^10.0.0",
    "brittle": "^3.18.0",
    "ignore-loader": "^0.1.2",
    "rollup": "^4.50.2",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
