{
  "name": "trac-crypto-api",
  "version": "0.0.9",
  "main": "index.js",
  "pear": {
    "name": "trac-crypto-api",
    "type": "terminal"
  },
  "scripts": {
    "build:browser": "rollup -c && cd test/browser/ && npm i && cd ../..",
    "test:node": "brittle-node -t 120000 test/unit/all.test.js",
    "test:bare": "brittle-bare -t 120000 test/unit/all.test.js",
    "test:browser": "cd test/browser && npm run test:browser",
    "test:all": "npm run test:node && npm run test:browser"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "util": {
      "bare": "bare-utils",
      "default": "util"
    },
    "crypto": {
      "bare": "bare-crypto",
      "default": "crypto"
    },
    "node:crypto": {
      "bare": "bare-crypto",
      "default": "crypto"
    }
  },
  "dependencies": {
    "@metamask/key-tree": "^10.1.1",
    "@tracsystems/blake3": "^0.0.7",
    "b4a": "1.6.7",
    "bare-fs": "^4.0.2",
    "bech32": "^2.0.0",
    "bip39-mnemonic": "^2.2.0",
    "sodium-javascript": "^0.8.0",
    "sodium-universal": "^5.0.1",
    "vite": "^7.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "brittle": "^3.18.0",
    "rollup": "^4.50.2",
    "supertest": "^7.1.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
