{
  "name": "trac-crypto-api",
  "version": "0.0.1",
  "main": "index.js",
  "type": "module",
  "pear": {
    "name": "trac-crypto-api",
    "type": "terminal"
  },
  "jest": {
    "testMatch": [
      "**/test/api/**/*.test.js"
    ],
    "testRunner": "jest-circus"
  },
  "c8": {
    "all": true,
    "include": [
      "server.js",
      "api/**/*.js",
      "modules/**/*.js"
    ]
  },
  "scripts": {
    "start": "node server.js",
    "test:node": "brittle-node -t 120000 test/all.test.js",
    "test:bare": "brittle-bare -t 120000 test/all.test.js",
    "test:api": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --watchAll --testTimeout=30000",
    "test:api:coverage": "c8 --reporter=html --check-coverage --lines 80 --functions 80 --branches 80 --all node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --testTimeout=30000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@tracsystems/blake3": "^0.0.6",
    "b4a": "1.6.7",
    "bare-fs": "^4.0.2",
    "bech32": "^2.0.0",
    "bip39-mnemonic": "^2.2.0",
    "express": "^5.1.0",
    "sodium-universal": "^5.0.1"
  },
  "devDependencies": {
    "brittle": "^3.18.0",
    "c8": "^10.1.3",
    "jest": "^30.0.5",
    "supertest": "^7.1.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
