{
  "name": "trac-crypto-api",
  "version": "0.0.5",
  "main": "index.js",
  "pear": {
    "name": "trac-crypto-api",
    "type": "terminal"
  },
  "scripts": {
    "build:browser": "rollup -c",
    "build:rn": "webpack --config webpack.config.rn.js",
    "build": "rm -rf dist && npm run build:browser && npm run build:rn",
    "test:node": "brittle-node -t 120000 test/unit/all.test.js",
    "test:bare": "brittle-bare -t 120000 test/unit/all.test.js",
    "test:browser": "cd test/browser && npm run test:browser",
    "test:all": "npm run test:node && npm run test:bare && npm run test:browser"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "util": {
      "bare": "bare-utils",
      "default": "util"
    },
    "crypto": {
      "bare": "bare-crypto",
      "default": "crypto"
    },
    "node:crypto": {
      "bare": "bare-crypto",
      "default": "crypto"
    }
  },
  "dependencies": {
    "@metamask/key-tree": "^10.1.1",
    "@tracsystems/blake3": "^0.0.7",
    "b4a": "1.6.7",
    "bare-fs": "^4.0.2",
    "bech32": "^2.0.0",
    "bip39-mnemonic": "^2.2.0",
    "sodium-universal": "^5.0.1",
    "sodium-javascript": "^0.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "babel-loader": "^10.0.0",
    "brittle": "^3.18.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "ignore-loader": "^0.1.2",
    "rollup": "^4.50.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
